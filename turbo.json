{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["packages/**/dist"]
    },
    "deploy": {
      "dependsOn": ["build", "lint"]
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}